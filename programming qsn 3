 //////////////////////
#include <iostream>
#include <cstdlib>
using namespace std;
namespace NS {
    int abs(int n) {
        if (n < -128) return 0;
        if (n > 127) return 0;
        if (n < 0) return -n;
        return n;
    }
}

int main() {
    double x, y, z;
    
    cin >> x;
    cin >> y;
    cin >> z;
    
    cout << NS::abs(x) << " "
    << NS::abs(y) << " "
    << NS::abs(z) << " "
     <<  endl;

    cout <<NS::abs(x) << " "
    << NS::abs(y) << " "
    << NS::abs(z) << " "
    << endl;

    return 0;
}
